  },
  "homepage": "https://github.com/marianabetancur18/protractor-workshop-2021#readme",
  "devDependencies": {
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.11.4",
    "jasmine-awesome-report": "^0.1.0",
    "protractor": "^7.0.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "jasmine-spec-reporter": "^7.0.0",
